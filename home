(function ($) {
  $(document).ready(function () {
    var $searchFormMobile = $('#search-form-tour-mobile'),
      $input = $('#odx-mobile-search__input'),
      searchURLPortal = searchUrl.replace('&', '&');

    $searchFormMobile.attr("action", searchURLPortal);
  });
})(jQuery);

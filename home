<div id="skipToNavAndMainContentContainer" class="hidden">
    <button id="skip-to-main-nav" alt='Skip to Navigation'>
        Skip to Navigation
    </button>
    <button id="skip-to-main-content" alt='Skip to Main Content'>
        Skip to Main Content
    </button>
</div>

<script>
function handleSkipToNavContent( keyBoardEvent ) {
    var selector = null;
    if ( !keyBoardEvent || (keyBoardEvent && keyBoardEvent.which == 13) ) {
        $( '.awesome-navigator' ).attr('tabindex', 0);
        $( '.awesome-navigator' ).focus();
    } else if (keyBoardEvent && keyBoardEvent.which === 27) {
        $( '#skip-to-main-nav' ).blur();
    }
    return false;
}
function handleSkipToMainContent( keyBoardEventContent ) {
    var selectorNext = null;
    if ( !keyBoardEventContent || (keyBoardEventContent && keyBoardEventContent.which == 13) ) {
        if ( $( '.core-main-banner__heading' ).length > 0 ) {
            selectorNext = '.core-main-banner__heading';
        } else if ( $( '.odx-full-banner__heading' ).length > 0 ) {
            selectorNext = '.odx-full-banner__heading';
        } else if ( $( '.ohid-full-banner__title' ).length > 0 ) {
            selectorNext = '.ohid-full-banner__title';
        } else if ( $( '.odx-content__title' ).length > 0 ) {
            selectorNext = '.odx-content__title';
        } else if ( $( '.odx-main-content' ).length > 0 ) {
            selectorNext = '.odx-main-content';
        } else {
            selectorNext = '.content';
        }
        if ( selectorNext && $( selectorNext ).length ) {
            $( selectorNext ).attr('tabindex', 0);
            $( selectorNext ).focus();
        }
    } else if ( keyBoardEventContent && keyBoardEventContent.which === 27 ) {
        $( '#skip-to-main-content' ).blur();
    }
    return false;
}
$( document ).ready(function() {
    $( 'body' ).prepend( '<div class="accessibility-overlay-cover"></div>' );
    $( '#skip-to-main-content' ).appendTo( '.accessibility-overlay-cover' );
    $( '#skip-to-main-nav' ).appendTo( '.accessibility-overlay-cover' );
    $( '#skip-to-main-content, #skip-to-main-nav' ).focus(function () {
        $( 'body' ).addClass( 'accessibility-overlay' );
    });
    $( '#skip-to-main-content, #skip-to-main-nav' ).blur(function () {
        $( 'body' ).removeClass( 'accessibility-overlay' );
    });

    $( '#skip-to-main-nav' ).on('keyup', function ( Event ) {
        handleSkipToNavContent( Event );
    });
    $( '#skip-to-main-nav' ).click(function ( Event ) {
        handleSkipToNavContent( null );
    });
    $( '#skip-to-main-content' ).on('keyup', function ( Event ) {
        handleSkipToMainContent( Event );
    });
    $( '#skip-to-main-content' ).click(function ( Event ) {
        handleSkipToMainContent( null );
    });
});
</script>


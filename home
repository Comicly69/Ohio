<script type="text/javascript">
  (function($) {
    $(document).ready(function() {

      $("#mobile-menu-btn").click(function(){
        $("#mobile-menu").toggle();      
      });

      $("#mobile-close-btn").click(function(){
        $("#mobile-menu").toggle();      
      });

      $("#explore-menu-button").hover(function(){
        $(this).children("#dropdown-menu").slideDown(100);
      }, function(){
        $(this).children("#dropdown-menu").stop().slideUp(100);
      });

      $("#things-menu-button").hover(function(){
        $(this).children("#dropdown-menu").slideDown(100);
      }, function(){
        $(this).children("#dropdown-menu").stop().slideUp(100);
      });

    });
  })(jQuery);
</script>


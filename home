<script type="text/javascript">
  (function ($) {
    $(document).ready(function () {
      updateItineraryHeaderCount();
     })
  }(jQuery));

  function updateItineraryHeaderCount(){
    var itineraryListData = JSON.parse(localStorage.getItem('itineraryArray'));
    var itineraryListDataLength = "0";
    if(itineraryListData){
      itineraryListDataLength = itineraryListData.length;
    }
    $('#itineraryCount').text(itineraryListDataLength)
  }
</script>

  var host = location.host,
      pathObject = null,
      currentKey = null,
      pathname = window.location.pathname;

    pathObject = keysFullArray[host];
    if (pathObject != undefined) {
      currentKey = pathObject.value;
    }

    var cleanPathname = pathname.replace(/\/!ut(\w|\W)*/gi, "");
    cleanPathname = cleanPathname.replace(/\/$/, "");
    if(typeof searchQuery !== "undefined" && searchQuery != null ){
        cleanPathname += "?search_query=" + searchQuery;
    }
    if (currentKey != null) {

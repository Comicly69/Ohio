<!-- START: Ohio Design > Components > Site Builder > Global Components > ODX Desktop > Header > Bottom Right Injector Standard Elements > Desktop Bottom Right Injector Standard Search JavaScript -->
<script type="text/javascript">
(function ($) {
    $( document ).ready(function () {
        var $searchButton = $( "#search-button" ),
            $searchForm = $( "#search-form" ),
            $searchTop = $( ".odx-search" ),
            $closeSearch = $( ".odx-search__close" ),
            $input = $( ".odx-search .odx-search__input" ).first(),
            searchURLPortal = searchUrl.replace("&", "&");

        $( document ).on("click", function ( event ) {
            var $target = $( event.target ),
                isClickInside = $target.closest( $searchForm ).length,
                isSearchButtonClicked = $target.closest( $searchButton ).length;
            if ( !isClickInside && !isSearchButtonClicked ) {
                $searchTop.removeClass( "in" );
            }
        });

        $searchButton.on("click", function () {
            $searchTop.addClass( "in" );
            $input.focus();
        });

        $searchButton.on("keypress", function ( event ) {
            if ( event.key === "Enter" ) {
                $( this ).trigger( "click" );
            }
        });

        $closeSearch.on("click", function () {
            $searchTop.removeClass( "in" );
        });

        $closeSearch.on("keypress", function ( event ) {
            if ( event.key === "Enter" ) {
                $closeSearch.trigger( "click" );
            }
        });

        $( "#search-form" ).on("keyup keypress", function ( event ) {
            if ( event.key === "Enter" ) {
                event.preventDefault();
                return false;
            }
        });

        $input.on("keydown", function ( event ) {
            event.stopPropagation();
            if ( event.key === "Enter" ) {
                $searchForm.submit();
            }
            if ( event.key === "Escape" ) {
                $closeSearch.trigger( "click" );
            }
        });

        $( ".odx-search__loop-btn" ).on("keypress", function ( event ) {
            if ( event.key === "Enter" ) {
             $searchForm.submit();
            }
        });
        $searchForm.attr("action", searchURLPortal);

        $( "#js-go-help" ).attr("href", window.globalLinks.help);
    });
})( jQuery );
</script>
<!-- END: Ohio Design > Components > Site Builder > Global Components > ODX Desktop > Header > Bottom Right Injector Standard Elements > Desktop Bottom Right Injector Standard Search JavaScript -->


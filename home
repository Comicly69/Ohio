<!-- START: ODX Common Design > Components > Global Components > Alerts > Site Alerts -->
<div class="odx-site-alert odx-site-sitewide hidden">
    <div class="container">
        <div class="odx-site-alert-wrapper" role="alertdialog" id="dialog_alert" aria-describedby="desc_dialog">
            <i class="odx-site-alert__icon" aria-hidden="true"></i>
            <p class="odx-site-alert__label" id="desc_dialog" class="desc_dialog"></p>
            <a type="button" class="odx-site-alert__view-more sr-only ohio-button secondary " href="#top">
                <span class="no-remove-sr-only sr-only" id="sitewide-alert"></span>
                <span class="multilingual">learn-more</span>
            </a>
        </div>
    </div>
</div>
<script>
(function( $ ) {
    $( document ).on('ready', function () {
        // Translate labels
        //OHIO.ODX.actions.applyMultilingualLabels('.odx-site-alert__view-more', 'span.multilingual');
        OHIO.ODX.actions.applyMultilingualLabels('.odx-site-alert-wrapper', 'span.multilingual');
        // 1) Get all variables to support multilingual content
        var currentLanguage = OHIO.ODX.actions.getUserLanguageByCookie();
        // Get the library according to the user's language
        var wcmLibrary = (typeof window.WCMLibraries !== "undefined") ? window.WCMLibraries[currentLanguage] : window.WCMLibraries['en'];
        var virtualPortal = (window.portalContext !== 'none') ? window.portalContext + '/' : 'gov';
        // 2) Configure parameters for AJAX call
        var params = {
            'source':               'library',
            'srv':                  'cmpnt',
            'cmpntid':              '94474ec9-b9db-4529-80bf-28a6e5c875f4',
            'WCM_Page.ResetAll':    'TRUE',
            'location':             wcmLibrary + '/' + window.siteId + '/home/alerts'
        };
        var timestamp = new Date (),
            serviceURL = '/wps/wcm/connect/gov/odx%20common%20design/utility/context?' + $.param( params ),
            outageNotificationIsCurrent = (window.location.pathname.indexOf('/gov/') > -1)? window.location.pathname.split( "/gov/" )[1] : window.location.pathname,
            indexDash = outageNotificationIsCurrent.indexOf( "/" ),
            cleanURL = outageNotificationIsCurrent.substring(indexDash + 1, outageNotificationIsCurrent.length);
        if ( cleanURL.split( "/" )[0] != "outage-notification" ) {
            // 3) Run the AJAX call and do something with the response
            OHIO.ODX.actions.getAjaxDataFromURL( serviceURL ).then( function ( response ) {
                var alertData = JSON.parse( response );
                var $alert = $( '.odx-site-sitewide' );
                var siteId = window.siteId;
                document.getElementById( "sitewide-alert" ).innerHTML = "Learn about" + " " + siteId + " " + "current alert";
                if ( !$.isEmptyObject( alertData ) ) {
                    var $label = $alert.find( '.odx-site-alert__label' ),
                        $linkElement = $alert.find( '.odx-site-alert__view-more' ),
                        $linkElementText = $alert.find( '.odx-site-alert__view-more-text' ),
                        linkObject = jQuery( alertData.linkParameter ),
                        linkTarget = linkObject.attr( 'target' );
                    alertType = alertData.alertType.toLowerCase();
                    $label.html( alertData.body);
                    if( alertData.linkText ) {
                        $linkElementText.html( alertData.linkText );
                    }
                    if( alertData.url ) {
                        if( alertData.url.indexOf( "https://" ) == -1 && alertData.url.indexOf("http://") == -1 ) {
                            if ( alertData.url.indexOf( "/wps/wcm/connect" ) >= 0 || alertData.url.indexOf( "/wps/wcm/connect" ) >= 0 ) {
                                var urlPath = alertData.url.replace("/wps/wcm/connect/", "");
                                urlPath = urlPath.replace("/wps/wcm/connect/", "");
                                urlPath = urlPath.split( "/" );
                                urlPath = urlPath.slice(2, urlPath.length);
                                alertData.url = urlPath.join( "/" );
                            }
                            $linkElement.attr('href', "/wps/portal/gov/" + alertData.url);
                        } else {
                            $linkElement.attr('href', alertData.url);
                        }
                    } else {
                        $linkElement.remove();
                    }
                    $linkElement.attr('target', linkTarget);
                    $alert.addClass( 'odx-site-alert--' + alertType ); 
                    $alert.find( '.sr-only' ).removeClass( 'sr-only' );
                    $alert.find( '.no-remove-sr-only' ).addClass( 'sr-only' );
                    //To show alerts when ACTIVE and hide when HIDDEN
                    if ( alertData.status === "Active" ) {
                        $alert.removeClass( 'hidden' ); 
                    } else if ( alertData.status === "Hidden" ) {
                        if( window.location.href.indexOf( "?showalerts" ) >= 0 ) {
                            $alert.removeClass( 'hidden' );
                        }
                    }
                    //To append the alerts on content
                    if ( $( '.odx-site-statewide' ).length > 0) {
                        $( '.odx-alerts-wrapper' ).append( $alert );
                    } else {
                        $( '.content-body' ).prepend( $alert );
                    }
                    //$alert.show();
                } else {
                    $alert.remove();
                }
            },
            function ( error ) {
                console.error( error );
            });
        }
    });
})( jQuery );
</script>
<!-- END: ODX Common Design > Components > Global Components > Alerts > Site Alerts -->


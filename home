        currentTitle = $( 'head title' ).text();
    if ( $('.aw-main-header h1').html() === "Home" || currentTitle === siteName ) {
        $( '.aw-main-header h1' ).text( siteName + " | Ohio.gov" );
        $( 'head title' ).text( siteName + " | Ohio.gov") ;
    } else {
        $( 'head title' ).text( currentTitle + " | " + siteName );

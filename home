<script>
$( document ).ready(function() {
    $( '#aw-main-wrapper .header' ).append( $( '#oficialSiteDropdown' ) );
    $( '#oficialSite' ).on("click", function ( event ) {
        event.preventDefault();
        $( '.oficialSite__container' ).toggleClass( 'opened' );
        $( '.oficialSite__container' ).toggleClass( 'closed' );
        $( '.oficialSiteDropdown__container' ).toggleClass( 'opened' );
        $( '.oficialSiteDropdown__container' ).toggleClass( 'closed' );
        $( '.oficialSiteDropdown__container' ).slideToggle( 300 );
        $( '#aw-main-wrapper .header' ).toggleClass( 'fixed' );
        $( '#aw-main-wrapper .env' ).toggleClass( 'fixed' );
        $( '.oficialSite__icon' ).toggleClass( 'hidden' );
    });
    $( document ).on("click", function( event ){
        var $trigger = $( '#aw-main-wrapper .header' );
        if ( $trigger !== event.target && !$trigger.has( event.target ).length && $( '.oficialSiteDropdown__container' ).hasClass( 'opened' ) ) {
            $( '#oficialSite' ).click();
        }
    });
});
</script>

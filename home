        $customMegaMenu = $( '.iop_mobile-menu__mega-menu' ),
        $agencyLogo = $( '#aw-header-bottom-left .odx-logo' ),
        $mobileMenuToggleBtn = $( '#js-toggle-mobile-menu' ),
        $helpLink = $iopMobileMenu.find( '#js-go-help' );
    if ( $iopMobileMenu.length > 0 ) {
        $agencyLogo.clone().appendTo( $iopMobileNav );
        $agencyLogo.clone().prependTo( $iopMobileSearch );
        if ( $customMegaMenu.length > 0 ) {
            $customMegaMenu.appendTo( $iopMobileNav );
        } else {
            $mainNav.clone().appendTo( $iopMobileNav );
        }
        $iopMobileNav.find( 'li' ).first().removeClass( 'hidden' );
        $helpLink.attr("href", window.globalLinks.help);
        $mobileMenuToggleBtn.click(function( event ) {
            event.preventDefault();
            $mobileMenuToggleBtn.find( 'i' ).toggleClass( 'hidden' );
            $mobileMenuToggleBtn.find( '.icon-description' ).toggleClass( 'hidden' );
            $mobileMenuToggleBtn.parent( 'li' ).toggleClass( 'active' );
            $iopMobileNav.toggleClass( 'open' );
            $( 'body' ).toggleClass( 'noscroll' );
            if ( $mobileMenuToggleBtn.attr( 'aria-expanded' ) == "false" ) {
                $mobileMenuToggleBtn.attr('aria-expanded', "true");
                $iopMobileNav.attr('aria-expanded', "true");
            } else {
                $mobileMenuToggleBtn.attr('aria-expanded', "false");
                $iopMobileNav.attr('aria-expanded', "false");
            }
        });
        $helpLink.click(function( event ) {
            if ( $mobileMenuToggleBtn.attr( 'aria-expanded' ) == "true" ) {
                $mobileMenuToggleBtn.click();
            }
        });
    }
    var iopMobileMenu = $( '#iop_mobile-menu' ),
        $searchButton = $( '#search-button-mobile' ),
        $toggleNavButton = $( '#js-toggle-mobile-menu' ),
        $searchForm = iopMobileMenu.find( '#search-form-mobile' ),
        $searchSlider = iopMobileMenu.find( '.iop_mobile-menu__search' ),
        $closeSearch = iopMobileMenu.find( '.odx-search__close' ),
        $input = iopMobileMenu.find( '.odx-search__input' ),
        searchURLPortal = searchUrl.replace('&', '&');
    $( document ).on('click', function ( event ) {
        var $target = $( event.target ),
            isClickInside = $target.closest( $searchForm ).length,
            isSearchButtonClicked = $target.closest( $searchButton ).length;
        if ( !isClickInside && !isSearchButtonClicked && $searchSlider.hasClass( 'in' ) ) {
            $searchSlider.removeClass( 'in' );
            $( 'body' ).toggleClass( 'noscroll' );
        }
    });
    $searchButton.on('click', function () {
        // Closes mobile nav in case it was open
        if ( $toggleNavButton.attr( 'aria-expanded' ) == "true" ) {
            $toggleNavButton.click();
        }
        $searchSlider.addClass( 'in' );
        $input.focus();
        $( 'body' ).toggleClass( 'noscroll' );
    });
    $searchButton.on('keypress', function ( event ) {
        var keyCode = event.which;
        if ( keyCode == 13 ) {
            $searchButton.trigger( 'click' );
        }
    });
    $closeSearch.on('click', function () {
        $searchSlider.removeClass( 'in' );
        $( 'body' ).toggleClass( 'noscroll' );
    });
    $closeSearch.on('keypress', function ( event ) {
        var keyCode = event.which;
        if ( keyCode == 13 ) {
            $closeSearch.trigger( 'click' );
        }
    });
    $searchForm.attr("action", searchURLPortal);
});
</script>
<!-- END: Ohio Design > Components > Site Builder > Global Components > IOP Footer > IOP Mobile Menu -->

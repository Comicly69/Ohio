<script type="text/javascript">
  (function ($) {
    $(document).ready(function () {
      $.getJSON('https://www.instagram.com/ohio.findithere/?__a=1&format=jsonp&jsonp=?')
        .done((data) => {
console.log(data, 'data');
          if (data && data.graphql.user.edge_owner_to_timeline_media.edges.length === 0) {
            $('<p>No search results</p>').appendTo('#instagram-list');
          } else {
            var instagramFeedResults = data.graphql.user.edge_owner_to_timeline_media.edges.map(function (photo) {
              var linkHTML = "https://instagram.com/p/" + photo.node.shortcode;
              return '<div class="instagram-img-container" style="margin: 0 5px;position:relative;"><img loading="lazy" src="' + photo.node.thumbnail_src + '" alt="' + photo.node.accessibility_caption +'" title="" /><a class="instagram-overlay" href="' + linkHTML + '" title="" target="_blank" style="position: absolute;height: 100%;width: 100%;text-decoration: none;color: white;top: 0;display: none;align-items: center;justify-content: center;cursor: pointer;z-index: 2;background: rgba(0,0,0,0.5);"><i style="font-size:20px;font-weight:bold;margin:8px;" class="fal fa-heart"><span style="margin-left:4px;">' + photo.node.edge_liked_by.count + '</span></i><i style="font-size:20px;font-weight:bold;margin:8px;" class="fal fa-comment"><span style="margin-left:4px;">' + photo.node.edge_media_to_comment.count + '</span></i></a></div>';
            }).join('');
            $(instagramFeedResults).appendTo('.instagram-list');

            $(".instagram-list")
              .slick({
                infinite: true,
                arrows: true,
                dots: true,
                draggable: false,
                slidesToShow: 5,
                slidesToScroll: 1,
                responsive: [
                  {
                    breakpoint: 1200,
                    settings: {
                      slidesToShow: 3,
                      slidesToScroll: 1,
                    },
                  },
                  {
                    breakpoint: 992,
                    settings: {
                      slidesToShow: 2,
                      slidesToScroll: 1,
                    },
                  },
                  {
                    breakpoint: 580,
                    settings: {
                      slidesToShow: 1,
                      slidesToScroll: 1,
                    },
                  },
                ],
              })
         }
      });
    });
  })(jQuery);
</script>


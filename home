<script>
$( document ).ready(function() {
    var currentURL = window.location.href,
        environmentURLs = {
            auth        : 'dx-authoring.myohio.gov',
            dev         : 'dev.ohio.gov',
            qa          : 'qa.ohio.gov',
            idTest      : 'idtest.ohio.gov',
            stg         : 'stg.ohio.gov',
            idStage     : 'idstage.ohio.gov'
        },
        $envIndicator = $( '<div>' ).attr( 'id' , 'env').addClass( 'env hidden' );
    $( '#aw-main-wrapper > section' ).prepend( $envIndicator );
    if ( currentURL.indexOf( environmentURLs.auth ) > -1 ) {
        $envIndicator.addClass( 'env-auth' );
        $envIndicator.append( '<i class="far fa-globe-americas"></i>' );
        $envIndicator.append( '<span>IOP AUTH</span>' );
        $envIndicator.removeClass( 'hidden' );
    }
    if ( currentURL.indexOf( environmentURLs.dev ) > -1 ) {
        $envIndicator.addClass( 'env-dev' );
        $envIndicator.append( '<i class="far fa-globe-africa"></i>' );
        $envIndicator.append( '<span>IOP DEV</span>' );
        $envIndicator.removeClass( 'hidden' );
    }
    if ( currentURL.indexOf( environmentURLs.qa ) > -1 || currentURL.indexOf( environmentURLs.idTest ) > -1 ) {
        $envIndicator.addClass( 'env-qa' );
        $envIndicator.append( '<i class="far fa-globe-asia"></i>' );
        $envIndicator.append( '<span>IOP QA</span>' );
        $envIndicator.removeClass( 'hidden' );
    }
    if ( currentURL.indexOf( environmentURLs.stg ) > -1 || currentURL.indexOf( environmentURLs.idStage ) > -1 ) {
        $envIndicator.addClass( 'env-stg' );
        $envIndicator.append( '<i class="far fa-globe-europe"></i>' );
        $envIndicator.append( '<span>IOP STG</span>' );
        $envIndicator.removeClass( 'hidden' );
    }
});
</script>

<script type="text/javascript">
//Fix for static images and documents, so they work in other environments for testing
var fixImageLinks = function() {
    var docsHost = "ohio.org";
    $( 'img[src]' ).filter(function() {
        return this.src.toLowerCase().indexOf( '/static/uploads' ) >= 0;
    }).each( function(index, value) {
        value.src = value.src.replace(window.location.host, docsHost);
        value.src = value.src.replace('ohio.org', docsHost);
    });
}
$(document).ready(fixImageLinks);
</script>


<script type="text/javascript">
  (function($) {
    $(document).ready(function() {

      $("#top-featured-button").hover(function(){
        $("#top-featured-submenu").slideDown(100);
      }, function(){
        $("#top-featured-submenu").stop().slideUp(100);
      });

      $("#seasonal-adventures-button").hover(function(){
        $("#seasonal-adventures-submenu").slideDown(100);
      }, function(){
        $("#seasonal-adventures-submenu").stop().slideUp(100);
      });

      $("#seasons-button").hover(function(){
        $("#seasons-submenu").slideDown(100);
      }, function(){
        $("#seasons-submenu").stop().slideUp(100);
      });

      $("#regions-button").hover(function(){
        $("#regions-submenu").slideDown(100);
      }, function(){
        $("#regions-submenu").stop().slideUp(100);
      });

      $("#areas-button").hover(function(){
        $("#areas-submenu").slideDown(100);
      }, function(){
        $("#areas-submenu").stop().slideUp(100);
      });

    });
  })(jQuery);
</script>

<script type="text/javascript">
window.onscroll = function() {
    odxScroll();
};
function odxScroll() {
    if ( document.body.scrollTop > 30 || document.documentElement.scrollTop > 30 ) {
        $( ".navigation" ).addClass( "sticky-header" );
    } else {
        $( ".navigation" ).removeClass( "sticky-header" );
    }
}
// Add offsett when scrolling
window.addEventListener("hashchange", function () {
    var scrollY = window.scrollY;
    if ( window.scrollY == undefined ) {
        scrollY = window.pageYOffset;
    }
    if ( scrollY !== 0 && $( window ).width() < 992 ) {
        window.scrollTo( window.scrollX, window.scrollY - 150 );
    }
});
//Opens External Links in a New Window unless a target is already specified
var openExternalLinksInNewWindow = function( $container ) {
    $container.find('a[href^="http"]').each(function() {
        if ( $( this ).attr( "target" ) === null || typeof $( this ).attr( "target" ) === 'undefined' ) {
            $( this ).attr("target", "_blank");
        }
    });
}
openExternalLinksInNewWindow( $('#odx-main-content') );
// "New Window" warning for links
window.bindScreenReaderWindowLinks = function( containerId ) {
    if ( containerId === undefined || null ){  
        var $newWindowLinks = document.querySelectorAll( 'a[target="_blank"]' );
        bindNewWindowLinks( $newWindowLinks );
    }
    else {
        var $newWindowLinks = document.querySelectorAll( containerId + ' a[target="_blank"]' );
        bindNewWindowLinks( $newWindowLinks );
    }
}
function bindNewWindowLinks( $newWindowLinks ) {
    var newWindowAlert = '',
        currentAriaLabel = '';
    for (var i = 0, l = $newWindowLinks.length; i < l; i++) {
        currentAriaLabel = $newWindowLinks[i].getAttribute( "aria-label" );
        if ( currentAriaLabel == null ) {
            currentAriaLabel = $newWindowLinks[i].innerHTML.trim();
            if ( $newWindowLinks[i].getElementsByTagName( "img" ).length > 0 ) {
                currentAriaLabel = $newWindowLinks[i].getElementsByTagName( "img" )[0].getAttribute( "alt" );
                $newWindowLinks[i].setAttribute('aria-label', currentAriaLabel + ', This link will open in a new window');
            } else if ( currentAriaLabel.indexOf( '=' ) == -1 ) {
                $newWindowLinks[i].setAttribute('aria-label', currentAriaLabel + ', This link will open in a new window');
            } else if ( $( $newWindowLinks[i] ).html().indexOf( 'ohio-takeover-wrapper' ) > -1 ) {
                newWindowAlert = document.createElement( 'div' );
                newWindowAlert.className = 'sr-only';
                newWindowAlert.innerHTML = 'This link will open in a new window';
                $newWindowLinks[i].appendChild( newWindowAlert );
            } else if(currentAriaLabel.indexOf( 'fa fa-external-link' ) > 0){
                currentAriaLabel =  $.trim($(currentAriaLabel).first().text());
                $newWindowLinks[i].setAttribute('aria-label', currentAriaLabel + ', This link will open in a new window');
            } 
        } 
        else if ( currentAriaLabel != null && currentAriaLabel.indexOf( 'This link will open in a new window' ) < 0  ) {
            $newWindowLinks[i].setAttribute('aria-label', currentAriaLabel + ', This link will open in a new window');
        }
    }
}
//Updates ALT attribute of agency's logo
var updateSiteLogoAltAttribute = function() {
    var $headerContainer = document.querySelector( '#aw-header-bottom-left' );
    var $logos = $headerContainer.querySelectorAll( '.odx-logo img' );
    var urlSubtrings = window.location.href.split( "/" );
    var isHome = urlSubtrings.slice(-1);
    if ( isHome[0] === "home" || isHome[0] === siteId) {
        $logos.forEach( function( logo ){
            logo.alt = "Refresh home page";
        });
    } else {
        $logos.forEach( function( logo ){
            logo.alt = "Return to home";
        });
    }
};
//Absolute Positions Footer to anchor it to the bottom of the page when main container is too small
var adjustFooterPositioningDependingOnMainContainerHeight = function() {
    var footerMainContainer = $( '.single-column-footer' ),
        mainContentContainerHeight = $( '#singleColumn' ).height(),
        viewPortHeight = $( "body" ).innerHeight(),
        headerHeight = 140,
        footerHeight = footerMainContainer.height();
    if ( mainContentContainerHeight < (viewPortHeight - headerHeight - footerHeight) ) {
        footerMainContainer.css('position', 'absolute');
    } else {
        footerMainContainer.css('position', 'relative');
    }
};
//This utility can be used to obtain the Service URL to retrieve a WCM Component via AJAX
window.wcmAjaxUtils = {
    LibrarySettings: {
        virtualPortal: '',
        wcmLibrary: ''
    },
    prepareLibrary: function( ) {
        var currentLanguage = OHIO.ODX.actions.getUserLanguageByCookie();
        this.LibrarySettings.virtualPortal = (window.portalContext !== 'none') ? window.portalContext + '/' : 'gov';
        this.LibrarySettings.wcmLibrary = (typeof window.WCMLibraries !== "undefined") ? window.WCMLibraries[currentLanguage] : window.WCMLibraries['en'];
    },
    getAjaxParameters: function( cmpntid, location, category ) {
        this.prepareLibrary( );
        var locationPathArray = decodeURIComponent( location ).split( '/' );
        location = '';
        locationPathArray.forEach(function (element, index) {
            if ( index > 1 ) {
                location += "/" + element;
            }
        });
        var Params = {
            source: 'library',
            srv: 'cmpnt',
            cmpntid: cmpntid,
            location: this.LibrarySettings.wcmLibrary + '/' + location,
            category: category
        };
        return Params;
    },
    getServiceURL: function( cmpntid, location, category ) {
        var Params = this.getAjaxParameters(cmpntid, location, category);
        var serviceURL = '/wps/wcm/connect/' + this.LibrarySettings.virtualPortal + this.LibrarySettings.wcmLibrary + '/' + window.siteId + '?' + $.param( Params );
        return serviceURL;
    }
};
//This function parses Configuration Mapping Objects typically located at
//ODX Common Design > Content > Utility>  Site Configuration Mappings
function parseConfigurationMappingObject ( configurationObject ) {
    var parsedObject = JSON.parse( $('<div />').html( decodeURIComponent( configurationObject ) ).text() );
    for (var property in parsedObject) {
        parsedObject[ property ] = parsedObject[ property ]["value"];
    }
    return parsedObject;
}
// This line allows to send the parameters in normal QUERY STRING (by default we send by HEADERS)
OHIO.ODX.configuration.useQueryStringParams = true;
// This line sets the default state for this flag
OHIO.ODX.configuration.isUserAuthenticated = false;
// This line changes depending on the current agency
OHIO.ODX.configuration.currentAgency = window.siteId;
OHIO.ODX.configuration.defaultImage = '/wps/wcm/connect/gov/978c423a-316a-46d3-bad9-f3acf25b1141/oda-default-card-image.jpg?MOD=AJPERES&amp;CACHEID=ROOTWORKSPACE.Z18_M1HGGIK0N0JO00QO9DDDDM3000-978c423a-316a-46d3-bad9-f3acf25b1141-madEszK';
// Disable DEBUG log information
OHIO.ODX.configuration.debug = false;
// Apply multilingual labels for header right
OHIO.ODX.actions.applyMultilingualLabels('#aw-header-top-right', 'span.multilingual');
OHIO.ODX.actions.applyMultilingualLabels('#aw-header-bottom-right', 'span.multilingual');
OHIO.ODX.actions.applyMultilingualLabels('.top-search', 'input.multilingual', true);
$( '#aw-header-top-right' ).slideDown();
// Apply multilingual labels for the content
OHIO.ODX.actions.applyMultilingualLabels('#singleColumn', 'span.multilingual');
OHIO.ODX.actions.applyMultilingualLabels('.single-column-footer', 'span.multilingual');
OHIO.ODX.actions.applyMultilingualLabels('#aw-content-layout-two-columns', 'span.multilingual');
OHIO.ODX.actions.applyMultilingualLabels('#aw-content-layout-single-column', 'span.multilingual');
OHIO.ODX.actions.applyMultilingualLabels('#aw-content-large-column', 'span.multilingual');
OHIO.ODX.actions.applyMultilingualLabels('#twoColumnsRight','span.multilingual');
$( document ).on('ready', function() {
    //Checks if current user is authenticated
    const myPortalString = "/" + "wps" + "/" + "my" + "portal" + "/";
    if ( window.location.href.indexOf( myPortalString ) > -1 ) {
        OHIO.ODX.configuration.isUserAuthenticated = true;
    }
    // if no option is selected from the left nav, the first one is going to be active
    if( $( '.odx-navigation--left li' ).length > 0 && $( '.odx-navigation--left li.active' ).length == 0 ){
        $( '.odx-navigation--left li' )[0].className += " active";
    }
    bindScreenReaderWindowLinks();
    // Hide all separators <hr> for screen readers
    $( 'hr' ).attr('aria-hidden', 'true');
    // Remove "IBM Websphere portal" main region label
    $( '.wpthemeMainContent' ).removeAttr( "role" );
    $( '.wpthemeMainContent' ).removeAttr( "aria-label" );
    $( '.wpthemeMenuLoading' ).text( '' );
    updateSiteLogoAltAttribute();
    // change <title>2

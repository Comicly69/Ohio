<script type="text/javascript">
function getWCMProjectPath() {
    var wcmProjectID = "",
        wcmProjectPath = "";
    if ( window.location.href.indexOf('$project') > -1 ) {
        wcmProjectID = window.location.href.match(/.*\$project\/([a-zA-Z0-9\-\_]+).*/)[1];
        wcmProjectPath = '$project/' + wcmProjectID + '/';
    }
    return wcmProjectPath;
}
(function () {
    var wcmProjectPath = getWCMProjectPath(),
        globalLinks = {};
    globalLinks.login = '/wps/portal/gov/' + wcmProjectPath + siteId + '/login';
    globalLinks.help = '/wps/portal/gov/' + wcmProjectPath + siteId + '/help-center';
    globalLinks.createAccount = '/wps/portal/gov/registration';
    globalLinks.liveChat = '/wps/portal/gov/home/live-chat';
    globalLinks.homepageMainImage = '/wps/portal/gov/home/';
    globalLinks.home = '/wps/portal/gov/' + wcmProjectPath + siteId + '/home';
    // Add here all agencies that support a global laws and rules page
    globalLinks.lawsAndRules = {
        'odh' : '/wps/portal/gov/' + wcmProjectPath + siteId + '/health-rules-laws-and-forms'
    };
    window.globalLinks = globalLinks;
    // Create libraries names to use as parameters in the URLs or cookies 
    // to get the error messages depending on user language
    var WCMLibraries = {
        "en" : "Ohio Content English",
        "es" : "Ohio Content Spanish",
        "so" : "Ohio Content Somali",
        "zh" : "Ohio Content Simplified Chinese",
        "ar" : "Ohio Content Arabic"
    };
    window.WCMLibraries = WCMLibraries;
})();
</script>

    //Add sr-only class to header
    $('header.aw-main-header').addClass('sr-only');

    //Add navigation roles to Primary/Secondary Nav
    $('#aw-header-navigation ul').attr('role', 'navigation');
    $('#aw-header-navigation ul').attr('aria-label', 'Primary Navigation');

    $('.odx-navigation.odx-navigation--left').attr('role', 'navigation');
    $('.odx-navigation.odx-navigation--left').attr('aria-label', 'Secondary Navigation'); 

});
</script>
